<div id="header">
  <%= link_to 'Wikinimous', articles_path %>
  <%= link_to 'New Article', new_article_path %>
</div>
<div id="container">
  <ul>
    <% @articles.each do |article| %>
      <li>
        <%= link_to article.title, article_path(article.id) %>
        <%= article.content %>
      </li>
    <% end %>
  </ul>
</div>

